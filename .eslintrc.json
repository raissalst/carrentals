{
  "env": {
    "es2021": true,
    "node": true,
    "jest": true
  },
  "extends": ["airbnb-base", "prettier"],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": ["@typescript-eslint", "@typescript-prettier"],
  "rules": {
    "no-console": "off",
    "implicit-arrow-linebreak": "warn"
  }
}
